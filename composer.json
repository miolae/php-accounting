    "keywords": [
        "billing",
        "invoice",
        "finances",
        "miolae",
        "viktorprogger"
    ],
    "version": "0.0.1",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/miolae/php-accounting/issues?state=open",
        "source": "https://github.com/miolae/php-accounting"
    },
